{
    "name": "codingfoundry/laravel-preset",
    "description": "Coding Foundry Default UI Presets",
    "keywords": ["laravel", "preset", "turbolinks", "vue", "tailwindcss"],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "laravel/framework": "^7.0",
        "laravel/ui": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*8.0"
    },
    "authors": [
        {
            "name": "Taylor Maguire",
            "email": "taylor@codingfoundry.com"
        },
        {
            "name": "Garry Reimer",
            "email": "garry@codingfoundry.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CodingFoundry\\LaravelPreset\\": "src/"
        }
    },
    "autoload": {
        "psr-4": {
            "CodingFoundry\\LaravelPreset\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodingFoundry\\LaravelPreset\\LaravelPresetServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
